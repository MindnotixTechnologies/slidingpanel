<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
    <metadata>
        <id>DK.SlidingPanel</id>
        <version>1.1.5</version>
        <title>DK Sliding Panel Plugin for Xamarin.Forms</title>
        <summary>Allows for a panel to be slided out from bottom of the screen for Xamarin.Forms pages</summary>
        <description>Allows for a panel to be slided out from bottom of the screen for Xamarin.Forms pages.
			Here are all the layers of the sliding panel:
				-> (Optional) Panel Control Layer (Back Button / Favorite Button)
				-> (Optional) Picture Layer 
				-> Title Layer 
				-> Body Layer
				
			
        </description>
        <authors>Dae Won Kim</authors>
        <owners>Dae Won Kim</owners>
        <!--
        <iconUrl>https://github.com/aritchie/userdialogs/raw/master/icon.png</iconUrl>
        <licenseUrl>https://github.com/aritchie/userdialogs/LICENSE.md</licenseUrl>
        -->
        <projectUrl>https://github.com/dw2kim/slidingpanel</projectUrl>
        <requireLicenseAcceptance>false</requireLicenseAcceptance>
        <tags>xamarin.forms xam.pcl sliding panel plugin</tags>
        <dependencies>
            <dependency id="Xamarin.Forms" version="2.2.0.31" />
            <dependency id="reactiveui" version="6.5.2" />
        </dependencies>
		
